<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryRed</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkRed</item>
        <item name="colorAccent">@color/colorAccentRed</item>'
        <item name="android:colorBackground">@color/colorBackgroundRed</item>
        <item name="backgroundTint">@color/backgroundTintRed</item>
    </style>
    <style name="GreenTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimaryGreen</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkGreen</item>
        <item name="colorAccent">@color/colorAccentGreen</item>
        <item name="android:colorBackground">@color/colorBackgroundGreen</item>
        <item name="backgroundTint">@color/backgroundTintGreen</item>
    </style>

</resources>